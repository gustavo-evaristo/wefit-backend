// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model customers {
  id         String    @id @default(uuid())
  type       String
  name       String
  email      String    @unique
  phone      String
  cellPhone  String
  cpf        String
  cnpj       String
  zipCode    String
  city       String
  state      String
  district   String
  street     String
  number     String
  complement String?
  createdAt  DateTime  @default(now())
  updatedAt  DateTime? @updatedAt
}
